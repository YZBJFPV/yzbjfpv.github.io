    <!-- JavaScript - 60+动画全量控制 -->
    <script>
        // 注册GSAP插件
        gsap.registerPlugin(ScrollTrigger, TextPlugin);
        
        document.addEventListener('DOMContentLoaded', function() {
            // ========== 加载动画控制 (3秒) ==========
            setTimeout(() => {
                const loader = document.getElementById('loader');
                gsap.to(loader, { 
                    opacity: 0, 
                    visibility: 'hidden',
                    duration: 1,
                    ease: "power3.out",
                    onComplete: () => {
                        loader.style.display = 'none';
                        initAllAnimations();
                    }
                });
            }, 3000);
            
            // ========== 动态渲染合作伙伴信息 (精准映射所有位置) ==========
            const renderPartnerInfo = () => {
                // 主名称
                document.getElementById('partner-name').textContent = partnerConfig.name;
                // 简介
                document.getElementById('partner-intro').textContent = partnerConfig.intro;
                // 微信按钮文本
                document.getElementById('partner-wx-text').textContent = `微信：${partnerConfig.wx}`;
                // 电话按钮文本
                document.getElementById('partner-phone-text').textContent = `电话：${partnerConfig.phone}`;
                // 合作说明
                document.getElementById('partner-cooperate').querySelector('p').textContent = partnerConfig.cooperateDesc;
                // 弹窗微信显示
                document.getElementById('partner-wx-show').textContent = partnerConfig.wx;
                // 招募说明
                document.getElementById('recruit-desc').innerHTML = recruitDesc;
                // 电话链接
                document.querySelector('a[href^="tel:"]').href = `tel:${partnerConfig.phone}`;
                
                // 验证所有映射
                console.log('配置映射完成:', partnerConfig);
            };
            renderPartnerInfo();

            // ========== 初始化60+动画 ==========
            function initAllAnimations() {
                // 1. GSAP滚动触发动画
                gsap.utils.toArray('.core-card').forEach((card, index) => {
                    gsap.from(card, {
                        y: 100,
                        opacity: 0,
                        scale: 0.9,
                        rotationX: 15,
                        duration: 1.2,
                        delay: index * 0.3,
                        ease: "power4.out",
                        scrollTrigger: {
                            trigger: card,
                            start: "top 80%",
                            end: "top 20%",
                            toggleActions: "play reverse play reverse"
                        }
                    });
                });
                
                // 2. 文字逐字动画
                gsap.utils.toArray('.text-animate-1, .text-animate-2, .text-animate-3').forEach((text, index) => {
                    gsap.from(text, {
                        text: { value: "" },
                        duration: 2,
                        delay: index * 0.2,
                        ease: "power2.out",
                        scrollTrigger: {
                            trigger: text,
                            start: "top 80%",
                            toggleActions: "play none none reverse"
                        }
                    });
                });
                
                // 3. 按钮悬停动画增强
                gsap.utils.toArray('.animate-btn').forEach(btn => {
                    btn.addEventListener('mouseenter', () => {
                        gsap.to(btn, { 
                            scale: 1.05, 
                            y: -5, 
                            rotation: 1,
                            boxShadow: "0 15px 30px rgba(22,93,255,0.3)",
                            duration: 0.5,
                            ease: "back.out(1.7)"
                        });
                    });
                    
                    btn.addEventListener('mouseleave', () => {
                        gsap.to(btn, { 
                            scale: 1, 
                            y: 0, 
                            rotation: 0,
                            boxShadow: "0 6px 20px rgba(22, 93, 255, 0.15)",
                            duration: 0.5,
                            ease: "back.in(1.7)"
                        });
                    });
                    
                    btn.addEventListener('click', () => {
                        gsap.to(btn, { 
                            scale: 0.95, 
                            duration: 0.2,
                            ease: "power2.in",
                            onComplete: () => {
                                gsap.to(btn, { scale: 1, duration: 0.3, ease: "power2.out" });
                            }
                        });
                    });
                });
                
                                // 4. 装饰元素3D动画
                gsap.utils.toArray('.decor-orbit').forEach((orbit, index) => {
                    gsap.to(orbit, {
                        rotationY: 360,
                        duration: 15 + index * 5,
                        repeat: -1,
                        ease: "none"
                    });
                    
                    gsap.to(orbit, {
                        y: `random(-50, 50)`,
                        x: `random(-50, 50)`,
                        duration: 8 + index * 3,
                        repeat: -1,
                        yoyo: true,
                        ease: "power1.inOut"
                    });
                });
                
                // 5. 装饰圆圈流体动画
                gsap.utils.toArray('.decor-circle').forEach((circle, index) => {
                    gsap.to(circle, {
                        scale: `random(1, 1.2)`,
                        duration: 20 + index * 5,
                        repeat: -1,
                        yoyo: true,
                        ease: "power2.inOut"
                    });
                    
                    gsap.to(circle, {
                        rotation: 360,
                        duration: 40 + index * 10,
                        repeat: -1,
                        ease: "none"
                    });
                    
                    gsap.to(circle, {
                        opacity: `random(0.3, 0.8)`,
                        duration: 8 + index * 2,
                        repeat: -1,
                        yoyo: true,
                        ease: "power1.inOut"
                    });
                });
                
                // 6. 粒子元素随机动画
                gsap.utils.toArray('.decor-particle').forEach((particle, index) => {
                    gsap.to(particle, {
                        x: `random(-200, 200)`,
                        y: `random(-200, 200)`,
                        duration: 15 + index * 4,
                        repeat: -1,
                        yoyo: true,
                        ease: "power1.inOut"
                    });
                    
                    gsap.to(particle, {
                        rotation: `random(0, 720)`,
                        duration: 10 + index * 2,
                        repeat: -1,
                        ease: "none"
                    });
                    
                    gsap.to(particle, {
                        scale: `random(0.5, 1.5)`,
                        duration: 5 + index * 1,
                        repeat: -1,
                        yoyo: true,
                        ease: "power1.inOut"
                    });
                    
                    gsap.to(particle, {
                        opacity: `random(0.1, 0.8)`,
                        duration: 3 + index * 0.5,
                        repeat: -1,
                        yoyo: true,
                        ease: "power1.inOut"
                    });
                });
                
                // 7. 导航栏滚动动画
                const nav = document.querySelector('.main-nav');
                gsap.to(nav, {
                    scrollTrigger: {
                        trigger: document.body,
                        start: "top 0",
                        end: "bottom bottom",
                        scrub: 1
                    },
                    backgroundColor: "rgba(255,255,255,0.98)",
                    boxShadow: "0 8px 30px rgba(0,0,0,0.1)",
                    duration: 0.5
                });
                
                // 8. 悬浮按钮3D旋转动画
                const floatBtn = document.querySelector('.nav-float-btn');
                gsap.to(floatBtn, {
                    rotationY: 360,
                    duration: 20,
                    repeat: -1,
                    ease: "none"
                });
                
                gsap.to(floatBtn, {
                    rotationX: 360,
                    duration: 25,
                    repeat: -1,
                    ease: "none",
                    delay: 2
                });
                
                // 9. 弹窗入场动画增强
                const modalBase = document.querySelector('.modal-base');
                const navModal = document.querySelector('.nav-modal');
                
                // 微信弹窗入场
                function animateModalIn(modal) {
                    gsap.from(modal.querySelector('.modal-content'), {
                        scale: 0.8,
                        opacity: 0,
                        rotationX: 20,
                        rotationY: -10,
                        y: -50,
                        duration: 0.8,
                        ease: "back.out(2)"
                    });
                    
                    gsap.from(modal.querySelector('.modal-content > *'), {
                        opacity: 0,
                        y: 20,
                        stagger: 0.1,
                        duration: 0.6,
                        delay: 0.3,
                        ease: "power2.out"
                    });
                }
                
                // 弹窗退场动画
                function animateModalOut(modal) {
                    gsap.to(modal.querySelector('.modal-content'), {
                        scale: 0.9,
                        opacity: 0,
                        rotationX: -10,
                        rotationY: 5,
                        y: 30,
                        duration: 0.5,
                        ease: "back.in(1.5)"
                    });
                }
                
                // 10. 文字流光动画
                gsap.utils.toArray('.text-gradient').forEach((text, index) => {
                    gsap.to(text, {
                        backgroundPosition: "200% center",
                        duration: 8 + index * 2,
                        repeat: -1,
                        ease: "power1.inOut"
                    });
                });
                
                // 11. 页面滚动视差效果
                gsap.utils.toArray('main > div').forEach((section, index) => {
                    gsap.to(section, {
                        y: (i, target) => -window.scrollY * 0.1,
                        scrollTrigger: {
                            trigger: section,
                            start: "top bottom",
                            end: "bottom top",
                            scrub: 1
                        }
                    });
                });
                
                // 12. 鼠标跟随粒子效果
                const particleContainer = document.getElementById('particle-container');
                let particles = [];
                
                // 创建100个跟随粒子
                for (let i = 0; i < 100; i++) {
                    const particle = document.createElement('div');
                    particle.classList.add('decor-particle');
                    particle.style.width = `${Math.random() * 8 + 2}px`;
                    particle.style.height = particle.style.width;
                    particle.style.left = `${Math.random() * 100}%`;
                    particle.style.top = `${Math.random() * 100}%`;
                    particle.style.opacity = Math.random() * 0.5 + 0.1;
                    particle.style.backgroundColor = `rgba(${Math.random() * 38 + 22}, ${Math.random() * 93 + 93}, ${Math.random() * 255 + 255}, ${Math.random() * 0.3 + 0.1})`;
                    particleContainer.appendChild(particle);
                    particles.push(particle);
                }
                
                // 鼠标移动跟随
                document.addEventListener('mousemove', (e) => {
                    particles.forEach((particle, index) => {
                        gsap.to(particle, {
                            x: `+=${(e.movementX * (index % 5 + 1)) / 10}`,
                            y: `+=${(e.movementY * (index % 5 + 1)) / 10}`,
                            duration: 0.5 + index * 0.01,
                            ease: "power2.out"
                        });
                    });
                });
                
                // 13. 页脚元素呼吸动画
                gsap.utils.toArray('footer p, footer a').forEach((el, index) => {
                    gsap.to(el, {
                        opacity: [0.8, 1, 0.8],
                        scale: [0.98, 1, 0.98],
                        duration: 4 + index * 0.5,
                        repeat: -1,
                        ease: "power1.inOut"
                    });
                });
                
                // 14. 卡片内部元素入场动画
                gsap.utils.toArray('.core-card > div > *').forEach((el, index) => {
                    gsap.from(el, {
                        opacity: 0,
                        y: 30,
                        duration: 0.8,
                        delay: index * 0.1,
                        ease: "power3.out",
                        scrollTrigger: {
                            trigger: el,
                            start: "top 90%",
                            toggleActions: "play none none reverse"
                        }
                    });
                });
                
                // 15. 3D卡片旋转效果
                document.querySelectorAll('.core-card').forEach(card => {
                    card.addEventListener('mousemove', (e) => {
                        const rect = card.getBoundingClientRect();
                        const x = (e.clientX - rect.left) / rect.width - 0.5;
                        const y = (e.clientY - rect.top) / rect.height - 0.5;
                        
                        gsap.to(card, {
                            rotationY: x * 10,
                            rotationX: -y * 10,
                            duration: 0.5,
                            ease: "power2.out"
                        });
                        
                        // 内部元素反向旋转
                        gsap.to(card.querySelectorAll('h1, p, button'), {
                            rotationY: -x * 5,
                            rotationX: y * 5,
                            duration: 0.5,
                            ease: "power2.out"
                        });
                    });
                    
                    card.addEventListener('mouseleave', () => {
                        gsap.to(card, {
                            rotationY: 0,
                            rotationX: 0,
                            duration: 0.8,
                            ease: "back.out(1.5)"
                        });
                        
                        gsap.to(card.querySelectorAll('h1, p, button'), {
                            rotationY: 0,
                            rotationX: 0,
                            duration: 0.8,
                            ease: "back.out(1.5)"
                        });
                    });
                });
                
                // 16. 滚动条进度动画
                window.addEventListener('scroll', () => {
                    const scrollProgress = window.scrollY / (document.body.scrollHeight - window.innerHeight);
                    
                    // 导航栏背景透明度随滚动变化
                    gsap.to('.main-nav', {
                        opacity: 1 - scrollProgress * 0.2,
                        duration: 0.1
                    });
                    
                    // 悬浮按钮大小随滚动变化
                    gsap.to('.nav-float-btn', {
                        scale: 1 - scrollProgress * 0.1,
                        duration: 0.1
                    });
                    
                    // 装饰元素颜色随滚动变化
                    gsap.utils.toArray('.decor-orbit').forEach((orbit, index) => {
                        const hue = 210 + scrollProgress * 60;
                        orbit.style.background = `linear-gradient(135deg, hsl(${hue}, 100%, 50%), hsl(${hue + 30}, 100%, 60%))`;
                    });
                });
                
                // 17. 按键交互震动动画
                document.addEventListener('keydown', (e) => {
                    if (e.key === ' ' || e.key === 'Enter' || e.key === 'ArrowUp' || e.key === 'ArrowDown') {
                        gsap.to('.core-card', {
                            y: [0, -5, 0],
                            rotation: [0, 1, -1, 0],
                            duration: 0.3,
                            ease: "power2.out"
                        });
                    }
                });
                
                // 18. 窗口大小变化动画
                window.addEventListener('resize', () => {
                    gsap.to('.core-card', {
                        scale: 0.98,
                        duration: 0.2,
                        onComplete: () => {
                            gsap.to('.core-card', {
                                scale: 1,
                                duration: 0.5,
                                ease: "back.out(1.5)"
                            });
                        }
                    });
                });
                
                // 19. 图片加载完成动画
                document.querySelectorAll('img').forEach(img => {
                    if (img.complete) {
                        gsap.from(img, {
                            scale: 0.8,
                            opacity: 0,
                            duration: 0.8,
                            ease: "back.out(1.5)"
                        });
                    } else {
                        img.addEventListener('load', () => {
                            gsap.from(img, {
                                scale: 0.8,
                                opacity: 0,
                                duration: 0.8,
                                ease: "back.out(1.5)"
                            });
                        });
                    }
                });
                
                // 20. 音频可视化效果（备用）
                try {
                    const audioCtx = new (window.AudioContext || window.webkitAudioContext)();
                    const oscillator = audioCtx.createOscillator();
                    oscillator.type = 'sine';
                    oscillator.frequency.setValueAtTime(440, audioCtx.currentTime);
                    
                    // 绑定点击音效
                    document.querySelectorAll('button, a').forEach(el => {
                        el.addEventListener('click', () => {
                            const gainNode = audioCtx.createGain();
                            oscillator.connect(gainNode);
                            gainNode.connect(audioCtx.destination);
                            gainNode.gain.setValueAtTime(0.1, audioCtx.currentTime);
                            gainNode.gain.exponentialRampToValueAtTime(0.001, audioCtx.currentTime + 0.3);
                            oscillator.start();
                            oscillator.stop(audioCtx.currentTime + 0.3);
                            
                            // 同步视觉动画
                            gsap.to(el, {
                                scale: 0.95,
                                duration: 0.1,
                                onComplete: () => {
                                    gsap.to(el, {
                                        scale: 1.05,
                                        duration: 0.1,
                                        onComplete: () => {
                                            gsap.to(el, {
                                                scale: 1,
                                                duration: 0.1
                                            });
                                        }
                                    });
                                }
                            });
                        });
                    });
                } catch (e) {
                    console.log('音频API不可用，跳过音频可视化动画');
                }
                
                console.log('60+动画初始化完成！');
            }

            // ========== 弹窗交互逻辑 ==========
            // 微信弹窗控制
            const wxModal = document.getElementById('wx-modal');
            const wxLink = document.getElementById('wx-link');
            const partnerWxBtn = document.getElementById('partner-wx-btn');
            const officialWxBtn = document.getElementById('official-wx-btn');
            const modalClose = document.querySelector('.modal-close');
            
            // 打开微信弹窗
            function openWxModal() {
                wxModal.classList.add('active');
                document.body.style.overflow = 'hidden';
                // 触发弹窗入场动画
                if (typeof initAllAnimations === 'function') {
                    const modalContent = wxModal.querySelector('.modal-content');
                    gsap.from(modalContent, {
                        scale: 0.8,
                        opacity: 0,
                        rotationX: 20,
                        y: -50,
                        duration: 0.8,
                        ease: "back.out(2)"
                    });
                }
            }
            
            // 关闭微信弹窗
            function closeWxModal() {
                const modalContent = wxModal.querySelector('.modal-content');
                gsap.to(modalContent, {
                    scale: 0.9,
                    opacity: 0,
                    rotationX: -10,
                    y: 30,
                    duration: 0.5,
                    ease: "back.in(1.5)",
                    onComplete: () => {
                        wxModal.classList.remove('active');
                        document.body.style.overflow = '';
                    }
                });
            }
            
            // 绑定微信弹窗事件
            wxLink.addEventListener('click', (e) => { 
                e.preventDefault(); 
                openWxModal(); 
            });
            
            partnerWxBtn.addEventListener('click', openWxModal);
            
            officialWxBtn.addEventListener('click', () => {
                document.getElementById('partner-wx-show').textContent = 'YGWFK2024';
                openWxModal();
            });
            
            modalClose.addEventListener('click', closeWxModal);
            
            wxModal.addEventListener('click', (e) => {
                if (e.target === wxModal) closeWxModal();
            });
            
            // 导航弹窗控制
            const navFloatBtn = document.querySelector('.nav-float-btn');
            const navModal = document.getElementById('nav-modal');
            const closeNavModal = document.getElementById('close-nav-modal');
            
            // 打开导航弹窗
            function openNavModal() {
                navModal.classList.add('active');
                document.body.style.overflow = 'hidden';
                // 触发导航弹窗入场动画
                const navContent = navModal.querySelector('.nav-content');
                gsap.from(navContent, {
                    scale: 0.9,
                    opacity: 0,
                    rotationY: -20,
                    x: -30,
                    duration: 0.8,
                    ease: "back.out(2)"
                });
                
                // 导航项逐次入场
                gsap.from(navContent.querySelectorAll('a'), {
                    opacity: 0,
                    x: -20,
                    stagger: 0.1,
                    duration: 0.6,
                    delay: 0.3,
                    ease: "power2.out"
                });
            }
            
            // 关闭导航弹窗
            function closeNavModalFunc() {
                const navContent = navModal.querySelector('.nav-content');
                gsap.to(navContent, {
                    scale: 0.95,
                    opacity: 0,
                    rotationY: 10,
                    x: 20,
                    duration: 0.5,
                    ease: "back.in(1.5)",
                    onComplete: () => {
                        navModal.classList.remove('active');
                        document.body.style.overflow = '';
                    }
                });
            }
            
            // 绑定导航弹窗事件
            navFloatBtn.addEventListener('click', openNavModal);
            closeNavModal.addEventListener('click', closeNavModalFunc);
            
            navModal.addEventListener('click', (e) => {
                if (e.target === navModal) closeNavModalFunc();
            });
            
            // 导航链接点击关闭弹窗
            document.querySelectorAll('.nav-content a').forEach(link => {
                link.addEventListener('click', (e) => {
                    if (link.getAttribute('href') === '#') {
                        e.preventDefault();
                    }
                    closeNavModalFunc();
                });
            });
            
            // ========== 点击波纹动画 ==========
            document.querySelectorAll('button, a, .nav-float-btn').forEach(el => {
                el.addEventListener('click', function(e) {
                    // 创建波纹元素
                    const ripple = document.createElement('span');
                    const rect = this.getBoundingClientRect();
                    const size = Math.max(rect.width, rect.height);
                    
                    ripple.style.width = ripple.style.height = `${size}px`;
                    ripple.style.left = `${e.clientX - rect.left - size/2}px`;
                    ripple.style.top = `${e.clientY - rect.top - size/2}px`;
                    ripple.style.position = 'absolute';
                    ripple.style.borderRadius = '50%';
                    ripple.style.background = 'rgba(255,255,255,0.3)';
                    ripple.style.transform = 'scale(0)';
                    ripple.style.animation = 'ripple1 0.8s ease-out';
                    ripple.style.pointerEvents = 'none';
                    ripple.style.zIndex = '10';
                    ripple.style.transition = 'all 0.8s ease';
                    
                    // 确保父元素相对定位
                    if (this.style.position === '' || this.style.position === 'static') {
                        this.style.position = 'relative';
                    }
                    
                    this.appendChild(ripple);
                    
                    // 自动移除波纹元素
                    setTimeout(() => {
                        ripple.remove();
                    }, 800);
                });
            });
            
            // ========== 平滑滚动 ==========
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function(e) {
                    e.preventDefault();
                    const targetId = this.getAttribute('href');
                    if (targetId === '#') return;
                    
                    const target = document.querySelector(targetId);
                    if (target) {
                        gsap.to(window, {
                            scrollTo: {
                                y: target.offsetTop - 80,
                                autoKill: false
                            },
                            duration: 1.2,
                            ease: "power4.out"
                        });
                    }
                });
            });
            
            // 页面加载完成后初始化所有动画
            window.addEventListener('load', () => {
                if (typeof initAllAnimations === 'function') {
                    initAllAnimations();
                }
                
                // 页面载入完成动画
                gsap.from(document.body, {
                    opacity: 0,
                    duration: 1,
                    ease: "power2.out"
                });
            });
        });
    </script>
</body>
</html>
